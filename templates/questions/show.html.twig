{% extends 'base.html.twig' %}
{% block title %}Question: {{ question }} {% endblock %}
{% block body %}
<h1> {{ question }}</h1>

<div>
    Eventually there will be text!
</div>

<h2> Answer({{ answers|length }}) </h2>
<ul>
    {% for answer in answers %}
        <li>
            {{ answer }}
        </li>
    {% endfor%}
</ul>
{% endblock %}